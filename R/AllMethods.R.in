mkformula<-function(dims2){
	if(length(dims2)==1){
		form<-as.formula(paste(c("",sapply((dims2), function(x) paste("`",x, "`", sep = ""))), collapse = "~"))
	}else{
		form<-as.formula(paste(sapply((dims2),function(x)paste("`",x,"`",sep="")),collapse="~"))
	}
	return(form)
}
